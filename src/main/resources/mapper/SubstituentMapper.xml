<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.subway.mapper.SubstituentMapper">
    <select id="getSubstituentList" resultType="map">
        select
                sbs_id,
                sbs_name_kor,
                sbs_name_eng,
                sbs_use_yn,
                sbs_cal,
                sbs_desc,
                sbs_cate_id,
                sbs_img_id
            from tbl_subway_substituent
    </select>




</mapper>